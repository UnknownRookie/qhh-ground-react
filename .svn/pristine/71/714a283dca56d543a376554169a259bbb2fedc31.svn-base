import Search from '../pages/search'
import { Routes, Route, HashRouter,Navigate} from "react-router-dom";

const baseRouter = () => (
    <HashRouter>
        <Routes>
            <Route path='/' element={<Navigate to="/search"/>}>
           
            </Route>
            <Route path='/search' element={<Search />}></Route>
        </Routes>
    </HashRouter>
)
export default baseRouter